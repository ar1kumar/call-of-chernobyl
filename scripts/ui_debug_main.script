class "debug_ui_attach" (CUIScriptWnd)
function debug_ui_attach:__init(owner, binds) super()
	self:SetWndRect				(Frect():set(0,0,1024,768))
	self:Enable					(true)

	local xml					= CScriptXmlInit()
